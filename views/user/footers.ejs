<script>
  //<![CDATA[ 
  $(window).load(function () {
    $("#slider-range").slider({
      range: true,
      min: 0,
      max: 400000,
      values: [50000, 160000],
      slide: function (event, ui) {
        $("#amount").val("₹" + ui.values[0] + " - ₹" + ui.values[1]);
        // Call a function to filter the products based on the selected price range
        filterProducts(ui.values[0], ui.values[1]);
      }
    });
    $("#amount").val("₹" + $("#slider-range").slider("values", 0) + " - ₹" + $("#slider-range").slider("values", 1));
  });

  function filterProducts(minPrice, maxPrice) {
    $(".product-men").each(function() {
      var productPrice = parseInt($(this).find(".money").text().trim().substring(1)); // Extract the price and remove the currency symbol
      if (productPrice >= minPrice && productPrice <= maxPrice) {
        $(this).show(); // Show the product if it falls within the selected price range
      } else {
        $(this).hide(); // Hide the product if it does not fall within the selected price range
      }
    });
  }
</script>
